    </dependency>
    
  		<dependency>
        	<groupId>javax.cache</groupId> <!-- JSR-107 API-->
        	<artifactId>cache-api</artifactId>
    	</dependency>
    
		<dependency>
			<groupId>ch.qos.logback</groupId>
			<artifactId>logback-classic</artifactId>
		</dependency>
		<dependency>
			<groupId>org.slf4j</groupId>
			<artifactId>jul-to-slf4j</artifactId>
